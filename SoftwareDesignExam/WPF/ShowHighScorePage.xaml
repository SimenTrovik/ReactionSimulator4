<Page x:Class="SoftwareDesignExam.WPF.ShowHighScorePage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:SoftwareDesignExam.WPF"
      mc:Ignorable="d" 
      d:DesignHeight="800" d:DesignWidth="1600"
      Title="ShowHighScorePage"
      Background="White">

    <Grid>
        <TextBlock Foreground="Black" x:Name="HighScoreText" HorizontalAlignment="Center" Margin="0,40,0,0" Text="HighScore!" TextWrapping="Wrap" VerticalAlignment="Top" FontSize="48" FontWeight="Bold"/>
        <TextBlock x:Name="highScoreListBlock" Margin="570,131,570,131" FontSize="36"></TextBlock>
        <Button Content="Back"
                x:Name="BackButton"
                Foreground="Black"
                FontSize="26"
                FontWeight="Medium" Margin="0,698,0,0" Width="200" Height="50" HorizontalAlignment="Center" VerticalAlignment="Top">
            <Button.Style>
                <Style TargetType="Button">
                    <Setter Property="Cursor" Value="Hand"/>
                    <EventSetter Event="Click" Handler="BackToMeny" />
                </Style>
            </Button.Style>

        </Button>
    </Grid>
</Page>
